# MQTT Cloud Broker
CLOUD_HOST=mqtt://localhost:1883
CLOUD_CREDENTIALS={"username":"your_mqtt_username","password":"your_mqtt_password"}

# Duration in milliseconds to collect data from MQTT broker
# Recommended: 60000 (60 seconds) to ensure all devices have time to publish
# Minimum: 10000 (10 seconds) for testing
COLLECTOR_DURATION=60000

OCPP_MYSQL_HOST=localhost
OCPP_MYSQL_PORT=3306
OCPP_MYSQL_DATABASE=ocpp_db
OCPP_MYSQL_USER=your_db_username
OCPP_MYSQL_PASSWORD=your_db_password

# Test mode: set device ID to only create points for that device (leave empty to process all)
# device ID can be not exist, it show logs but won't do anithing
# Leave empty or remove this line to process all devices
TEST_DEVICE_ID=aaaa-bbbb-cccc-dddd
